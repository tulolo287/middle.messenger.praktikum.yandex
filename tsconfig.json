{
  "compilerOptions": {
  "outDir": "dist",
  "target": "es2016",
  "declaration": false,
  "module": "commonjs",
  "strictNullChecks": true,
  "sourceMap": true,
  "noImplicitAny": true,    
  "noUnusedLocals": true,   
  "noUnusedParameters": true, 
  "strict": true,                                     
  "skipLibCheck": true,
  "lib": ["ES2020", "DOM", "DOM.Iterable"]
 }
} 
